<?xml version="1.0" encoding="utf-8"?>
<tests xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://hl7.org/fhirpath/tests" xsi:schemaLocation="http://hl7.org/fhirpath/tests ../../testSchema/testSchema.xsd"
	name="CqlNullologicalOperatorsTest" reference="https://cql.hl7.org/09-b-cqlreference.html#nullological-operators-3" version="1.0">
	<capability code="nullological-operators" />
	<group name="Coalesce" version="1.0">
		<capability code="nullological-operators" />
		<test name="CoalesceANull" version="1.0">
			<capability code="nullological-operators" />
			<expression>Coalesce('a', null)</expression>
			<output>'a'</output>
		</test>
		<test name="CoalesceNullA" version="1.0">
			<capability code="nullological-operators" />
			<expression>Coalesce(null, 'a')</expression>
			<output>'a'</output>
		</test>
		<test name="CoalesceEmptyList" version="1.0">
			<capability code="nullological-operators" />
			<expression>Coalesce({})</expression>
			<output>null</output>
		</test>
		<test name="CoalesceListFirstA" version="1.0">
			<capability code="nullological-operators" />
			<expression>Coalesce({'a', null, null})</expression>
			<output>'a'</output>
		</test>
		<test name="CoalesceListLastA" version="1.0">
			<capability code="nullological-operators" />
			<expression>Coalesce({null, null, 'a'})</expression>
			<output>'a'</output>
		</test>
		<test name="CoalesceFirstList" version="1.0">
			<capability code="nullological-operators" />
			<expression>Coalesce({'a'},null, null)</expression>
			<output>{'a'}</output>
		</test>
		<test name="CoalesceLastList" version="1.0">
			<capability code="nullological-operators" />
			<expression>Coalesce(null, null, {'a'})</expression>
			<output>{'a'}</output>
		</test>
		<test name="DateTimeCoalesce" version="1.0">
			<capability code="nullological-operators" />
			<expression>Coalesce(null, null, DateTime(2012, 5, 18))</expression>
			<output>@2012-05-18T</output>
		</test>
		<test name="DateTimeListCoalesce" version="1.0">
			<capability code="nullological-operators" />
			<expression>Coalesce({ null, null, DateTime(2012, 5, 18) })</expression>
			<output>@2012-05-18T</output>
		</test>
		<test name="TimeCoalesce" version="1.0">
			<capability code="nullological-operators" />
			<expression>Coalesce(null, null, @T05:15:33.556)</expression>
			<output>@T05:15:33.556</output>
		</test>
		<test name="TimeListCoalesce" version="1.0">
			<capability code="nullological-operators" />
			<expression>Coalesce({ null, null, @T05:15:33.556 })</expression>
			<output>@T05:15:33.556</output>
		</test>
	</group>
	<group name="IsNull" version="1.0">
		<capability code="nullological-operators" />
		<test name="IsNullTrue" version="1.0">
			<capability code="nullological-operators" />
			<expression>IsNull(null)</expression>
			<output>true</output>
		</test>
		<test name="IsNullFalseEmptyString" version="1.0">
			<capability code="nullological-operators" />
			<expression>IsNull('')</expression>
			<output>false</output>
		</test>
		<test name="IsNullAlsoFalseAbcString" version="1.0">
			<capability code="nullological-operators" />
			<expression>IsNull('abc')</expression>
			<output>false</output>
		</test>
		<test name="IsNullAlsoFalseNumber1" version="1.0">
			<capability code="nullological-operators" />
			<expression>IsNull(1)</expression>
			<output>false</output>
		</test>
		<test name="IsNullAlsoFalseNumberZero" version="1.0">
			<capability code="nullological-operators" />
			<expression>IsNull(0)</expression>
			<output>false</output>
		</test>
	</group>
	<group name="IsFalse" version="1.0">
		<capability code="nullological-operators" />
		<test name="IsFalseFalse" version="1.0">
			<capability code="nullological-operators" />
			<expression>IsFalse(false)</expression>
			<output>true</output>
		</test>
		<test name="IsFalseTrue" version="1.0">
			<capability code="nullological-operators" />
			<expression>IsFalse(true)</expression>
			<output>false</output>
		</test>
		<test name="IsFalseNull" version="1.0">
			<capability code="nullological-operators" />
			<expression>IsFalse(null)</expression>
			<output>false</output>
		</test>
	</group>
	<group name="IsTrue" version="1.0">
		<capability code="nullological-operators" />
		<test name="IsTrueTrue" version="1.0">
			<capability code="nullological-operators" />
			<expression>IsTrue(true)</expression>
			<output>true</output>
		</test>
		<test name="IsTrueFalse" version="1.0">
			<capability code="nullological-operators" />
			<expression>IsTrue(false)</expression>
			<output>false</output>
		</test>
		<test name="IsTrueNull" version="1.0">
			<capability code="nullological-operators" />
			<expression>IsTrue(null)</expression>
			<output>false</output>
		</test>
	</group>
</tests>
