	<!-- CQL spec section 3.1.3 Literals:
	Null - The null literal (null) -->
	<group name="Null" version="1.0">
		<capability code="literals"/>
		<test name="Null" version="1.0">
			<capability code="literals"/>
			<expression>null</expression>
			<output>null</output>
		</test>
	</group>
