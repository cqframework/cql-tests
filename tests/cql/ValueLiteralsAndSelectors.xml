			<capability code="literals"/>
			<expression>false</expression>
			<output>false</output>
		</test>
		<test name="BooleanTrue" version="1.0">
			<capability code="literals"/>
			<expression>true</expression>
			<output>true</output>
		</test>
	</group>
	<!-- CQL spec section 3.1.3 Literals:
	Integer - Sequences of digits in the range 0..2^32-1 -->
